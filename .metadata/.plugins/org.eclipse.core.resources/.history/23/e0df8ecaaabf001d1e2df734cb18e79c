package basics;

public class Primeornot {

	public static void main(String[] args) {
		int n=7;
		int count=0;
		
		if(n<=1) {
			System.out.println("the number is not prime");
		}
		
		for(int i=1;i<=n/2;i++) {
			if(n%i==0) {
				count++;
			}
		}
		
		//if the number of facroes is greater than 1
		
		if(count>1) {
			System.out.println("The number is not prime");
		}else {
			System.out.println("the number is prime");
		}
	}
}
